# DESCRIPTION
PROJECT_NAME     = "Marsupilami"

# DOXYGEN
QUIET            = NO
SHORT_NAMES      = NO
CASE_SENSE_NAMES = YES
HIDE_SCOPE_NAMES = YES
OPTIMIZE_OUTPUT_JAVA   = YES
